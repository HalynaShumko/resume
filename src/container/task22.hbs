<main class="p-5">
    <div class="row g-3">
        <div class="col-12">
            {{#with page_header}}
            <h1 class="text-center text-uppercase">{{title}}</h1>
            <p class=" text-center text-uppercase">{{description}}</p>
            {{/with}}

            <br>

            {{#with section}}
            <div class=" card">
                <div class="card-body">
                    {{#with about_us}}
                    {{#with title_list}}
                    <div class="display-6 text-end">{{main}}</div>
                    <h2 class="text-end">{{sub}}</h2>
                    {{/with}}
                    {{#with button}}
                    <button class="btn btn-info">{{text}}</button>
                    {{/with}}
                    {{/with}}
                </div>
            </div>

            <br>

            <div class=" card">
                <div class="card-body">
                    {{#with advantage}}
                    <div class="display-5">{{title}}</div>
                    <br>
                    <div class=" card">
                        <div class="card-body">
                            {{#with efficiency}}
                            <div class="display-4">{{title}}</div>
                            <br>
                            <h6>{{sub_title}}</h6>
                            <p>{{description}}</p>
                            <button class="btn btn-outline-warning">{{button}}</button>
                            {{/with}}
                        </div>
                    </div>

                    <br>

                    <div class=" card">
                        <div class="card-body">
                            {{#with innovation}}
                            <div class="display-4 text-center">{{title}}</div>
                            <br>
                            <h6 class="text-center">{{sub_title}}</h6>
                            <p class="text-center">{{description}}</p>
                            <button class="btn btn-outline-warning">{{button}}</button>
                            {{/with}}
                        </div>
                    </div>

                    <br>

                    <div class=" card">
                        <div class="card-body">
                            {{#with collaboration}}
                            <div class="display-4 text-and">{{title}}</div>
                            <br>
                            <h6 class="text-and">{{sub_title}}</h6>
                            <p class="text-and">{{description}}</p>
                            <button class="btn btn-outline-warning">{{button}}</button>
                            {{/with}}
                        </div>
                    </div>
                    {{/with}}
                </div>
            </div>

            <br>

            <div class=" card">
                <div class="card-body">
                    {{#with agreement}}
                    <div class="display-5">{{title}}</div>
                    <br>
                    <div class=" card">
                        <div class="card-body">
                            {{#with info}}
                            <h5>{{sub_title}}</h5>
                            <div class=" card">
                                <div class="card-body">
                                    {{#with detail}}
                                    <p>{{text}}</p>
                                    <button class="btn btn-success">{{button}}</button>
                                    {{/with}}
                                </div>
                            </div>
                            {{/with}}

                        </div>
                    </div>
                    {{/with}}
                </div>
            </div>
            {{/with}}
        </div>
    </div>
</main>